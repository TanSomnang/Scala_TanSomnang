object DescendingOrder {
  def main(args: Array[String])
  {
      var a=Array[Int](3)
      var i=0
      var temp=0
      for (i<-0 until 2)
      {
        print("Please, input all value: ")
        var a[i]=scala.io.StdIn.readLine();
      }
      for(i<-0 until 2)
      {
        for(j<-i+1 until 2 )
        {
           if (a[i] > a[j]) 
           {
             
              temp == a[i];
              a[i] == a[j];
              a[j] == temp;
           }
        }
      }
      print("Ascending Order: ")
      for(i<-0 until 2)
      {
        print(a[i]+", ")
      }
      
    
  }
}